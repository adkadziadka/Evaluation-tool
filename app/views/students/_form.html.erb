<%= form_for @student do |f| %>
  <div class="form-group">
    <label>Student name</label>
    <%= f.text_field :full_name, placeholder: "Student\'s name", class: "form-control" %>
  </div>

  <div class="form-group">
    <label>Picture</label>
    <%= f.url_field :picture, placeholder: "image url", class: "form-control" %>
  </div>

  <div class="form-group">
    <label>Batch</label>
    <%= f.number_field :number, placeholder: "Batchnumber", class: "form-control" %>
  </div>

  <div class="actions">
    <%= f.submit "Save", class: "btn btn-primary" %>
  </div>
<% end %>